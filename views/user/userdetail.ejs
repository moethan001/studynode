<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>User details</h1>
    Name:<%=user1.name%><p>
    Email:<%=user1.email%><p>
        Password:<%=user1.password%><p>
        <a href="/users/userupdate/<%=user1._id%>"><button type="button" name="button">update</button></a>
        <button type="button" name="button" onclick="del()">Delete</button>
  </body>
  <script type="text/javascript">
  function del(){
    var con=confirm("Are you sure ok!!");
    if(con){
      location.href='/users/delete/<%=user1._id%>';
    }
  }

  </script>
</html>
